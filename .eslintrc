{
  parser: "babel-eslint",
  extends: "airbnb",
  env: {
    es6: true,
    mocha: true,
    browser: true
  },
  parserOptions: {
    sourceType: "module"
  },
  settings: {
    "import/resolver": "webpack"
  },
  rules: {
    "class-methods-use-this": 0,
    "react/forbid-prop-types": 0,
    "no-console": 0,
    "react/require-default-props": 0,
    "no-nested-ternary": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
  }
}
